{
  "expo": {
    "name": "Yoel Ganon",
    "slug": "yael-hodaya-fingel",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "buildNumber": "25",
      "supportsTablet": true,
      "bundleIdentifier": "app.rork.et-kviyat-torim-labatan-tziporanim",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "CFBundleDevelopmentRegion": "en",
        "CFBundleAllowMixedLocalizations": true,
        "NSPhotoLibraryUsageDescription": "האפליקציה זקוקה לגישה לתמונות כדי שתוכל לבחור ולהעלות תמונות לגלריה או לפרופיל.",
        "NSPhotoLibraryAddUsageDescription": "האפליקציה עשויה לשמור תמונות שצילמת לספריית התמונות שלך.",
        "NSCameraUsageDescription": "האפליקציה זקוקה לגישה למצלמה כדי לצלם תמונות להעלאה."
      },
      "jsEngine": "hermes"
    },
    "android": {
      "package": "com.hodayayael.mybooking",
      "versionCode": 5,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "intentFilters": [
        {
          "autoVerify": true,
          "action": "VIEW",
          "data": {
            "scheme": "https",
            "host": "rork.com"
          },
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "supportsRtl": false
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://rork.com/"
        }
      ],
      [
        "expo-notifications",
        {
          "color": "#ffffff"
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "locales": {
      "he": "./assets/locales/he.json"
    },
    "extra": {
      "router": {
        "origin": "https://rork.com/"
      },
      "eas": {
        "projectId": "f0c09635-7e73-4fc6-94e1-b0addf0ab9f3"
      },
      "locale": "en"
    }
  }
}
