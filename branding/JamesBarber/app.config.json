{
  "expo": {
    "name": "James Barber",
    "slug": "james-barber",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./branding/JamesBarber/icon.png",
    "scheme": "jamesbarber",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./branding/JamesBarber/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "splashScreen": {
      "hideAsync": true,
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "image": "./branding/JamesBarber/splash.png",
      "timeout": 3000,
      "autoHide": true,
      "fadeOutDuration": 200
    },
    "ios": {
      "buildNumber": "1",
      "supportsTablet": true,
      "bundleIdentifier": "com.bcode.jamesbarber",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "CFBundleDevelopmentRegion": "en",
        "CFBundleAllowMixedLocalizations": true,
        "NSCalendarsUsageDescription": "The app uses your calendar to add and manage haircut appointments you schedule.",
        "NSRemindersUsageDescription": "The app can create reminders for your upcoming haircut appointments.",
        "NSPhotoLibraryUsageDescription": "The app needs access to photos to select and upload images to the gallery or profile.",
        "NSPhotoLibraryAddUsageDescription": "The app may save photos you've taken to your photo library.",
        "NSCameraUsageDescription": "The app needs access to the camera to take photos for upload."
      },
      "jsEngine": "hermes"
    },
    "android": {
      "package": "com.jamesbarber.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./branding/JamesBarber/icon.png",
        "backgroundColor": "#ffffff"
      },
      "intentFilters": [
        {
          "autoVerify": true,
          "action": "VIEW",
          "data": {
            "scheme": "https",
            "host": "jamesbarber.com"
          },
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "supportsRtl": false
    },
    "web": {
      "favicon": "./branding/JamesBarber/icon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://jamesbarber.com/"
        }
      ],
      [
        "expo-notifications",
        {
          "color": "#ffffff"
        }
      ],
      "expo-web-browser",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
          "origin": "https://jamesbarber.com/"
      },
      "locale": "en",
      "CLIENT": "JamesBarber",
      "logo": "./branding/JamesBarber/logo.png",
      "logoWhite": "./branding/JamesBarber/logo-white.png"
    }
  }
}
