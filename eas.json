{
  "cli": {
    "version": ">= 16.13.4",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "node": "20.19.4",
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "RCT_NEW_ARCH_ENABLED": "1"
      }
    },
    "preview": {
      "node": "20.19.4",
      "distribution": "internal",
      "env": {
        "RCT_NEW_ARCH_ENABLED": "1"
      }
    },
    "production": {
      "node": "20.19.4",
      "autoIncrement": true,
      "env": {
        "CLIENT": "KetyCooper",
        "ENV_FILE": "branding/KetyCooper/.env",
        "POD_INSTALL_ARGS": "--repo-update",
        "RCT_NEW_ARCH_ENABLED": "1"
      },
      "cache": { "disabled": false }
    },
    "JamesBarber": {
      "node": "20.19.4",
      "autoIncrement": true,
      "env": {
        "CLIENT": "JamesBarber",
        "ENV_FILE": "branding/JamesBarber/.env",
        "POD_INSTALL_ARGS": "--repo-update",
        "RCT_NEW_ARCH_ENABLED": "1"
      }
    },
    "clientB": {
      "node": "20.19.4",
      "autoIncrement": true,
      "env": {
        "CLIENT": "clientB",
        "ENV_FILE": "branding/clientB/.env",
        "RCT_NEW_ARCH_ENABLED": "1"
      }
    },
    "KetyCooper": {
      "node": "20.19.4",
      "autoIncrement": true,
      "env": {
        "CLIENT": "KetyCooper",
        "ENV_FILE": "branding/KetyCooper/.env",
        "POD_INSTALL_ARGS": "--repo-update",
        "RCT_NEW_ARCH_ENABLED": "1"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "itaybenyair99@gmail.com"
      }
    },
    "JamesBarber": {
      "ios": {
        "appleId": "itaybenyair99@gmail.com",
        "ascAppId": "6752813865"
      }
    },
    "clientB": {
      "ios": {
        "appleId": "itaybenyair99@gmail.com"
      }
    },
    "KetyCooper": {
      "ios": {
        "appleId": "itaybenyair99@gmail.com"
      }
    }
  }
}
