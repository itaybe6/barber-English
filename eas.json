{
  "cli": {
    "version": ">= 16.13.4",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "NPM_CONFIG_LEGACY_PEER_DEPS": "true",
        "EAS_BUILD_NODE_PACKAGE_MANAGER": "npm",
        "RCT_NEW_ARCH_ENABLED": "0"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "NPM_CONFIG_LEGACY_PEER_DEPS": "true",
        "EAS_BUILD_NODE_PACKAGE_MANAGER": "npm",
        "RCT_NEW_ARCH_ENABLED": "0"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "CLIENT": "JamesBarber",
        "ENV_FILE": "branding/JamesBarber/.env",
        "POD_INSTALL_ARGS": "--repo-update",
        "NPM_CONFIG_LEGACY_PEER_DEPS": "true",
        "EAS_BUILD_NODE_PACKAGE_MANAGER": "npm",
        "RCT_NEW_ARCH_ENABLED": "0"
      },
      "cache": {
        "disabled": false
      }
    },
    "JamesBarber": {
      "autoIncrement": true,
      "env": {
        "CLIENT": "JamesBarber",
        "ENV_FILE": "branding/JamesBarber/.env",
        "POD_INSTALL_ARGS": "--repo-update",
        "NPM_CONFIG_LEGACY_PEER_DEPS": "true",
        "EAS_BUILD_NODE_PACKAGE_MANAGER": "npm"
      }
    },
    "clientB": {
      "autoIncrement": true,
      "env": {
        "CLIENT": "clientB",
        "ENV_FILE": "branding/clientB/.env",
        "NPM_CONFIG_LEGACY_PEER_DEPS": "true",
        "EAS_BUILD_NODE_PACKAGE_MANAGER": "npm"
      }
    }
  },
  "submit": {
    "JamesBarber": {
      "ios": {
        "appleId": "itaybenyair99@gmail.com",
        "ascAppId": "6752813865"
      }
    },
    "clientB": {
      "ios": {
        "appleId": "itaybenyair99@gmail.com"
      }
    }
  }
}
